      Bow=5, DoubleJump OR Dash
      Launch
    advanced, WestHollow.UpperDrainLeverPulled:
      Bow=5, Bash, Grenade=1  # Involves bashing off a mosquito, no idea on bow requirement
      Bow=5, Bash, Damage=20  # Involves bashing off a mosquito, no idea on bow requirement
  state WestHollow.PurpleDoorOpen:
    base, WestHollow.UpperDrainLeverPulled:
      Bow=1, DoubleJump, Dash
      Bow=1, Launch
      Bow=1, Bash, Grenade=1
    advanced, WestHollow.UpperDrainLeverPulled:
      Bow=1, Glide, DoubleJump OR Dash

  pickup WestHollow.CrusherHC:
    base:
      WestHollow.UpperDrainLeverPulled OR DoubleJump OR Dash OR Glide OR Launch OR Bow=1
      Bash, Grenade=1
    advanced:
      Damage=10 #dboost through poison water
  pickup WestHollow.FarLeftEX:
    base, WestHollow.LowerDrainLeverPulled, WestHollow.RockPuzzleSolved, EnemyObstacle:
      Bow=4, DoubleJump, Dash  # awkward pickup
      Launch
  pickup WestHollow.RockPuzzleEX:
    base: WestHollow.LowerDrainLeverPulled
    advanced, WestHollow.UpperDrainLeverPulled: 
      Damage=100
  pickup WestHollow.HiddenEC:
    base: WestHollow.LowerDrainLeverPulled, BreakWall=10
  
  conn TwillenShop:
