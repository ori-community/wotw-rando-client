      WestHollow.UpperDrainLeverPulled OR Bow=1 OR DoubleJump OR Glide OR Launch
      Bash, Grenade=1
  conn MarshPastOpher.BowArea:
    base, EnemyObstacle:
      DoubleJump OR Launch
      Bash, Grenade=1, WestHollow.UpperDrainLeverPulled OR Bow=1 OR Glide
      Bash, Grenade=2
  conn WestHollow.HollowDrainLower:
    base: WestHollow.LowerDrainLeverPulled
  conn EastHollow.Teleporter:
