  pickup MarshSpawn.ResilienceShard:
    moki: BreakWall=16
  pickup MarshSpawn.ResilienceOre:
    moki: BreakWall=16, Water
  pickup MarshSpawn.BashEC:
    moki: Bash OR Launch
    gorlek:
      SentryJump=1
      DoubleJump, TripleJump, Sword  # Use pogo of the slime to jump to the ledge
