region WestHollow:
  base:
    Danger=20
area WestHollow.Entrance: # TODO: coords n stuff 
  pickup WestHollow.CrusherHC: free

  conn MarshPastOpher.BowArea: 
    base: MarshPastOpher.EyestoneDoor

  conn WestHollow.HollowDrainUpper:
    base:
      DoubleJump, Bow=1 OR Dash OR Glide
      Dash, Glide
      Launch
    advanced:
      Bash, Glide  # bashglide off a slime

area WestHollow.HollowDrainUpper:  # (-178, -4238) To the left of the purple eye door
  state WestHollow.UpperDrainLeverPulled:  # Lever that drains the first section of the hollow
    base:
      Bow=1, DoubleJump OR Danger=10  # easy to clip the spikes on the pole
      Dash, DoubleJump  # Makes you Dash past a falling slime
      Launch
    advanced:
      DoubleJump, Damage=10  # One damage boost on the spikes
  state WestHollow.RockPuzzleSolved:  # Will be convenient to have this later on
    base, WestHollow.UpperDrainLeverPulled, Weapon:  # can't shoot properly with the skeeto
      Bow=6, DoubleJump OR Dash
      Bow=4, Launch  # since the traversal includes bowshots, the energy requirements here will be inaccurate. Shouldn't be relevant with Bow's cheap cost though, and it gives lenience on not solving the puzzle immediately
  state WestHollow.LowerDrainLeverPulled:  # Lever that drains the second section of the hollow - this state strictly includes also having pulled the upper lever
