    gorlek:
      Water, WaterDash, Bash, Glide
      Water, WaterDash, Launch, Glide
      Water, WaterDash, Launch, Damage=10, DoubleJump OR Dash OR Sword OR Hammer
      Water, WaterDash, Launch, DoubleJump, TripleJump OR Dash OR Sword OR Hammer
    unsafe:
      Water, WaterDash, Bash, Hammer, Damage=10  # dboost at the start, too precise for gorlek
      Water, WaterDash, Bash, Sword  # too precise for gorlek

  conn MarshSpawn.BurrowsEntry:
    moki: Bash, DoubleJump OR Dash OR Launch
