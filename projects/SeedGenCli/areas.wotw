      Water, Burrow, Bash, DoubleJump, Dash OR Glide
      Water, Burrow, Bash, Launch
    gorlek:
      Water, Burrow, Combat=Bat, DoubleJump OR Dash
      Water, Burrow, Damage=10, DoubleJump OR Dash
    unsafe:
      Water, Burrow, Sword OR Hammer

